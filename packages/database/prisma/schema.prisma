generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Url {
  id          String   @id @default(cuid())
  code        String   @unique
  originalUrl String
  qrSvg       String?
  hits        Int      @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
